doctype html
html
  head
    title Pug Embedded HTML Example
    script.
      function loadPage(url) {
        document.getElementById("embeddedPage").src = url;
      }
  body
    h1 Welcome to Pug Embedded Page
    button(onclick="loadPage('https://example.com')") Load Page
    iframe#embeddedPage(
      width="800"
      height="600"
      style="border: none;"
    )
